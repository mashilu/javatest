<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
  
    <servlet>
        <servlet-name>MyFirstServlet</servlet-name>
        <servlet-class>com.mhh.servletTest.MyFirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>My2Servlet</servlet-name>
        <servlet-class>com.mhh.servletTest.My2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyFirstServlet</servlet-name>
        <url-pattern>/MyFirstServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>My2Servlet</servlet-name>
        <url-pattern>/My2Servlet</url-pattern>
    </servlet-mapping>
</web-app>
